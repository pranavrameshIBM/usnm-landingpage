        script.
            document.addEventListener('DOMContentLoaded', function() {
                const newsTicker = document.querySelector('.news-ticker');
                const newsItems = document.querySelectorAll('.news-item');
                const prevBtn = document.getElementById('prevNews');
                const nextBtn = document.getElementById('nextNews');
                let currentIndex = 0;
                const totalItems = newsItems.length;
                let autoScrollInterval;
            
                function updateNewsPosition() {
                    newsTicker.style.transform = `translateX(-${currentIndex * 100}%)`;
                    prevBtn.disabled = currentIndex === 0;
                    nextBtn.disabled = currentIndex === totalItems - 1;
                }
            
                function nextSlide() {
                    if (currentIndex < totalItems - 1) {
                        currentIndex++;
                    } else {
                        currentIndex = 0; // Loop back to the first item
                    }
                    updateNewsPosition();
                }
            
                function prevSlide() {
                    if (currentIndex > 0) {
                        currentIndex--;
                        updateNewsPosition();
                    }
                }
            
                // Auto-scroll functionality
                function startAutoScroll() {
                    autoScrollInterval = setInterval(nextSlide, 5000); // Change slide every 3 seconds
                }
            
                function stopAutoScroll() {
                    clearInterval(autoScrollInterval);
                }
            
                // Event listeners
                prevBtn.addEventListener('click', () => {
                    stopAutoScroll();
                    prevSlide();
                    startAutoScroll();
                });
            
                nextBtn.addEventListener('click', () => {
                    stopAutoScroll();
                    nextSlide();
                    startAutoScroll();
                });
            
                // Pause auto-scroll when hovering over the ticker
                const tickerContainer = document.querySelector('.news-ticker-wrapper');
                tickerContainer.addEventListener('mouseenter', stopAutoScroll);
                tickerContainer.addEventListener('mouseleave', startAutoScroll);
            
                // Initialize
                updateNewsPosition();
                startAutoScroll(); // Start auto-scrolling
            });

